#include<stdio.h>
int main(){
    int i=0, sum=0;
    char a[100];
    scanf("%s", a);
    
    if(strlen(a)!=10)
    printf("Illegal ISBN");
    
    else{
     for(i=0; i<10; i++){
         sum+=((i+1)*(a[i] - 48));
     }
        if(sum%11==0)
        printf("Legal ISBN");
        else printf("Illegal ISBN");
        
    }
}
