#include<stdio.h>
int main(){

int Q, i, flag=0;
scanf("%d", &Q);
while(Q!=0){

char a[100];
int hash[26]={0};
scanf("%s", a);

flag=0;

for(i=0; a[i]!='\0'; i++)
hash[ a[i]-'a']++;

for(i=0; i<26; i++){
if(hash[i]==0){
    flag=1;
    break;
}
}

if(flag==1)
    printf("NO\n");
else
printf("YES\n");

Q--;
    }
}
