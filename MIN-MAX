#include<stdio.h>
int main(){
    //DECLARING VARIABLES AND SCANNING NUMBER OF TERMS IN ARRAY
    int n; scanf("%d", &n);
    int a[n];
    int i, min=999999, max=0, sum=0;
    for(i=0; i<n; i++){
        //SCANNING INDIVIDUAL INPUT
        scanf("%d", &a[i]);
        
        //SAVING MINIMUM OF ALL DIGITS IN VARIABLE MIN
        if(a[i]<min)
        min=a[i];
        
        //SAVING MAXIMUM OF ALL IN ONE VARIABLE MAX
        if(a[i]>max)
        max=a[i];
        
        //ADDING ALL NUMBERS INTO ONE VARIABLE
        sum+=a[i];
    }
    
    //MAXIMUM SUM = SUM - LEAST NUMBER,  MINIMUM NUMBER = SUM - LARGEST NUMBER
    printf("%d %d", sum-max, sum-min);
}
