#include<stdio.h>
int main(){
    
    int x=0, y=0, i;
    char a[100000];
    scanf("%s", a);
    
    for(i=0; i<strlen(a); i++){
        if(a[i]=='L')
            x=x-1;
        else if(a[i]=='R')
            x=x+1;
        else if(a[i]=='U')
            y=y+1;
        else if(a[i]=='D')
            y=y-1;
    }
    
    printf("%d %d", x,y);
    
}
