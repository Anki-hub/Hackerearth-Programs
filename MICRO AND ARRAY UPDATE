//(GIVEN NUMBER - THE LOWEST VALUE OF ARRAY) IS THE MINIMUM NUMBER OF STEPS REQUIRED
#include<bits/stdc++.h>
using namespace std;

int main(){
    int test_cases, num_of_elements,element ,min, target_num;
    cin>>test_cases;
    
    while(test_cases--){
        cin>>num_of_elements;
        cin>>target_num;
        min=1000000;
   
   //TO SAVE SOME SPACE, WE TAKE ONLY SINGLE VARIABLE AS THE ARRAY ELEMENTS NEED TO BE UTILISED ONLY ONCE, SO ONLY ONE VARIABLE WOULD WORK
   for(int i=0; i<num_of_elements; i++){
            cin>>element;
            if( element <= min )
            min=element;
        }
        if(min<target_num)
        cout<<target_num-min<<endl;
        else cout<<"0\n";
    }
}
