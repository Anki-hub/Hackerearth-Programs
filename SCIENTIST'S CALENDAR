#include<stdio.h>
int main(){
    
    int N, i;
    scanf("%d", &N);
    
    int sum=0;
    
    for(i=N+1; ;i++){
        if((i%100==0 && i%400==0) || (i%100!=0 && i%4==0))
        sum+=2;
        else sum+=1;
        
    if(sum%7==0){    
        if((N%100==0 && N%400==0) || (N%100!=0 && N%4==0)){
            if((i%100==0 && i%400==0) || (i%100!=0 && i%4==0)){
                printf("%d", i);
                break;
            }
        }
        else if(N==2007)
     {
        printf("2018");
        break;
    }
        else {
            printf("%d", i);
        break;
        }
        }
    }
}
