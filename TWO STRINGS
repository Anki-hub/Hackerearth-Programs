#include<stdio.h>
int main(){
    
    int N; 
    scanf("%d", &N);
    
    while(N!=0){
        
        char a[100000]={0};
        char b[100000]={0};
     
     int len1=0;
     int len2=0;
     int i=0, j=0, flag=0;
     int hasha[26]={0};
     int hashb[26]={0};
     
     scanf("%s", a);
     scanf("%s", b);
     
     len1 = strlen(a);
     len2 = strlen(b);
     
     if(len1!=len2)
     printf("NO\n");
     
     else{
         for(i=0; i<len1; i++){
             hasha[ a[i] - 'a' ]++;
             hashb[ b[i] - 'a' ]++;
         }
         
         for(i=0; i<26; i++){
             if(hasha[i] == hashb[i])
             flag=0;
             else
             {
                 flag=1;
             break;
             }
         }
         
         if(flag==0)
         printf("YES\n");
         else printf("NO\n");
         
     }
     
     N--;   
    }
}
