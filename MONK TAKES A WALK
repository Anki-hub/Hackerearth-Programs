#include<stdio.h>
#include<string.h>
int main(){
    int T; scanf("%d", &T);
    while(T){
    char a[100000];
    int count=0;
    scanf("%s", a);
    
    for(int i=0; a[i]!='\0'; i++){
    if(a[i]=='a' || a[i]=='e' || a[i]=='i' || a[i]=='o' || a[i]=='u' || a[i]=='A' || a[i]=='E' || a[i]=='I' || a[i]=='O' || a[i]=='U'){
count++;  
        
    }      
    }
    
    printf("%d\n", count);
T--;
    }    
}
