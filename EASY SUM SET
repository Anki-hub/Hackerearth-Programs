#include<bits/stdc++.h>
using namespace std;

int main(){
    
    int max_1 = INT_MIN, min_1 = INT_MAX, max_2 = INT_MIN, min_2 = INT_MAX;
    
    int size_a;
    
    cin>>size_a;
    
    int a[size_a];
    
    for(int i=0; i<size_a; i++)
    {
        cin>>a[i];
        
        if( a[i] < min_1)
            min_1 = a[i];
            
        if( a[i] > max_1 )
            max_1 = a[i];
        
    }    
    
    int size_b;
    
    cin>>size_b;
    
    int b[size_b];
    
    for(int i=0; i<size_b; i++)
    {
        cin>>b[i];
        
        if( b[i] < min_2 )
            min_2  = b[i];
            
        if( b[i] > max_2 )
            max_2 = b[i];
         
    }    
    
    bool flag = false;
    int count = 0;
    
    for( int i = abs( min_2 - min_1); i<= ( max_2 - max_1 ); i++ ){
        
        
        count = 0;
        
        for( int j = 0; j<size_a ; j++  ){
            
            flag = false;
            
            for( int k = 0; k<size_b; k++ ){
                if( a[j] + i == b[k] ){
                    flag = true;
                    break;
                    }
            }
            
            if(flag)
                count++;
            
        }
        if(count == size_a)
            cout<<i<<" ";
        
    }
    
    
}
