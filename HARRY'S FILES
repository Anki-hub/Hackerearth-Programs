#include<stdio.h>
int main(){
    char a[100000];
    scanf("%s", a);
    int add=0,ext=0, i=0;
    
    //To get last 
    for(i=strlen(a)-1; i>=0; i--)
    {
        if(a[i]==92){
        add = i;
        break;
        }
        if(a[i]=='.'){
        ext = i;
        }
    }

    //TO PRINT ADRESS
    printf("Path: ");
    for(i=0; i<=add; i++)
    printf("%c", a[i]);
    
    printf("\nFile name: ");
    
    //TO print name
    for(i=add+1 ; i<ext ; i++ )
    printf("%c", a[i]);
    
    printf("\nExtension: ");
    
    //To print extension
    for(i=ext+1; i<strlen(a); i++)
    printf("%c", a[i]);
    
    
}
