#include<stdio.h>
int main(){
    
    int N;
    int hash[1000]={0};
    scanf("%d", &N);
    int a[N], i=0, max=0, min=100, j=0, count=0;
    
    for(i=0; i<N; i++){
        scanf("%d", &a[i]);
        
        if(a[i]>max)
        max=a[i];
        
        if(a[i]<min)
        min=a[i];
    }
    
    for(i=0; i<N; i++){
        for(j=min+1; j<max; j++){
            
            if(hash[ a[i] ]==0){
            if( a[i] == j)
            count++;
            }
        }
            hash[ a[i] ]++;
    }
    if( count == (max-min-1))
    printf("YES");
    else printf("NO");
}
