#include<bits/stdc++.h>
using namespace std;

void display(int *array, int size){
    
    for(int i=0; i<size ; i++)
    cout<<*(array+i)<<" ";
    
    cout<<endl;
    
}

int main(){
    
    int *array, test_cases;
    cin>>test_cases;
    
    while(test_cases--){
        
        int size, i, pos, k, temp;
        cin>>size>>k;
        
        array = new int[size];
        for(int i=0; i<size; i++){
            cin>>temp;
            pos=(i+k)%size;
            *(array+pos)=temp;
        }
        
        display(array, size);
        
    }
    
}
