#include<stdio.h>
int main(){
    
    int T; scanf("%d", &T);
    while(T){
        
    long int n, i;
    scanf("%ld", &n);
    n=n-1;
    long int sum3=0, sum5=0, sum15=0;
    
    sum3 = ((n/3)*((2*3)+((n/3)-1)*3))/2;
    sum5 = ((n/5)*((2*5)+((n/5)-1)*5))/2;
    sum15 = ((n/15)*((2*15)+((n/15)-1)*15))/2;
    
    printf("%ld\n", (sum3+sum5)-sum15);
    
    T--;
    }
}
