#include<stdio.h>
int main(){
    int i, min;
    int N; scanf("%d", &N);
    char a[N];
    
    scanf("%s", a);
    
    int hash[26]={0};
    for(i=0; a[i]!='\0' ; i++){
        hash[ a[i] - 'a' ]++;
    }
    
//C
min = hash[2]/1;

//T 
    if(min > (hash[19]/1))
min = (hash[19]/1);

//K
    if(min > (hash[10]/1))
min = (hash[10]/1);

//A
if(min > (hash[0]/2))
min = (hash[0]/2);


//H 
    if(min > (hash[7]/2))
min = (hash[7]/2);

//E   
    if(min > (hash[4]/2))
min = (hash[4]/2);

//R   
  if(min > (hash[17]/2))
min = (hash[17]/2);

    printf("%d", min);
}
