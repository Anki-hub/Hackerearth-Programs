#include<stdio.h>
int main(){
    int flag=0, i;
    char s[9];
    scanf("%s", s);
    
    if(s[2]=='A' ||s[2]=='I' || s[2]=='O' || s[2]=='U' || s[2]=='Y' || s[2]=='E')
    flag=1;
    
    else if(((s[0]+s[1])%2!=0) || ((s[4]+s[5])%2!=0) || ((s[3]+s[4])%2!=0) || ((s[7]+s[8])%2!=0))
 flag=1;

if(flag==0)
printf("valid");
else printf("invalid");
    
}
