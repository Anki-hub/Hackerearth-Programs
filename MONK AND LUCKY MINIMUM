/*This can be done simply by using hashing, incrementing value's count by 1 in each appearance
and then checking for the lowest's frequency*/
#include<bits/stdc++.h>
using namespace std;

int main(){
    
    int test_cases;
    scanf("%d", &test_cases);
    
    while(test_cases--){
    int num, i=0;
    scanf("%d", &num);
    
    long int x, min=999999999, count=0;
    
    
    for(i=0; i<num; i++){
        scanf("%ld", &x);
       
        if(x<min)
        {
        min=x;
        count=1;
        }
        else if(x==min)
        count++;
    }
    
    if( count%2==1 )
    printf("Lucky\n");
    else printf("Unlucky\n");
    
    }    
}
