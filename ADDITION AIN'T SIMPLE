#include<stdio.h>
#include<string.h>
int main(){
    int T, i=0, l=0, m=0;
    scanf("%d", &T);
while(T!=0){
    
    char a[100000];

    scanf("%s", a);
        l = strlen(a);
    char b[l];

    for(i=0; i<l; i++){
            
            m=a[i]+a[l-i-1]-192;
            
            if(m!=26)
            {
                b[i] = m%26 + 96;
            if(b[i]==96)
            b[i]='z';
            }
            else if(m==26)
            b[i] =122;
            
    }
    b[l]='\0';
printf("%s\n", b);

    T--;
}
    
}
